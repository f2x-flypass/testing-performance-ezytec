            <stringProp name="script">import java.io.FileWriter
import java.io.BufferedWriter
import java.io.PrintWriter

// Variables de JMeter
String placa = vars.get(&quot;plateRE&quot;)
String ingresoId = vars.get(&quot;uniqueIDRE&quot;)
String response = prev.getResponseDataAsString()
int responseCode = prev.getResponseCode().toInteger()  // Obtener el código de respuesta
String responseMessage = prev.getResponseMessage()  // Obtener el mensaje de respuesta
// Ruta de los archivos de salida
String successfulTransactionsFile = &quot;C:/Users/alejandra.ospina/Documents/GitHub/testing-performance-ezytec/Ejecucion/Logs/crearIngresoExitoso.txt&quot;
String failedTransactionsFile = &quot;C:/Users/alejandra.ospina/Documents/GitHub/testing-performance-ezytec/Ejecucion/Logs/crearIngresoFallido.txt&quot;

// Verificar si la transacción fue exitosa
boolean isSuccess = prev.isSuccessful()

if (isSuccess) {
    // Si la transacción fue exitosa, guardar la placa y el ingresoId en el archivo correspondiente
    FileWriter fileWriter = new FileWriter(successfulTransactionsFile, true)
    BufferedWriter bufferedWriter = new BufferedWriter(fileWriter)
    PrintWriter printWriter = new PrintWriter(bufferedWriter)

    printWriter.println(&quot;Placa: &quot; + placa + &quot;, uniqueIDRE: &quot; + ingresoId)

    printWriter.close()
    bufferedWriter.close()
    fileWriter.close()
} else {
    // Si la transacción falló, guardar la placa y la respuesta del servidor en el archivo correspondiente
    FileWriter fileWriter = new FileWriter(failedTransactionsFile, true)
    BufferedWriter bufferedWriter = new BufferedWriter(fileWriter)
    PrintWriter printWriter = new PrintWriter(bufferedWriter)

    printWriter.println(&quot;Placa: &quot; + placa + &quot;, Response Code: &quot; + responseCode + &quot;, Response Message: &quot; + responseMessage + &quot;, Response: &quot; + response)

    printWriter.close()
    bufferedWriter.close()
    fileWriter.close()
}










