            <stringProp name="script">import groovy.json.JsonSlurper
import java.nio.file.Files
import java.nio.file.Paths
import java.nio.file.StandardOpenOption

// Ruta de los archivos donde se guardarán los resultados
String fileFalse = &quot;C:/Users/alejandra.ospina/Documents/GitHub/testing-performance-ezytec/Diseño/Data/automatic_payment_false.txt&quot;
String fileTrue = &quot;C:/Users/alejandra.ospina/Documents/GitHub/testing-performance-ezytec/Diseño/Data/automatic_payment_true.txt&quot;
String fileNotFound = &quot;C:/Users/alejandra.ospina/Documents/GitHub/testing-performance-ezytec/Diseño/Data/users_not_found.txt&quot;

// Función para escribir en el archivo
void writeFile(String path, String content) {
    File file = new File(path)
    file &lt;&lt; content + System.lineSeparator()
}

// Obtener la respuesta
String response = prev.getResponseDataAsString()
def jsonResponse = new JsonSlurper().parseText(response)

// Verificar y guardar según el valor de is_automatic_payment
if (jsonResponse.is_automatic_payment == false) {
    writeFile(fileFalse, jsonResponse.plate)
} else if (jsonResponse.is_automatic_payment == true) {
    writeFile(fileTrue, jsonResponse.plate)
} else {
    writeFile(fileNotFound, jsonResponse.message)
}
