            <stringProp name="script">import java.io.FileWriter;
import java.io.PrintWriter;
import java.io.File;
import groovy.json.JsonSlurper;

// Variables de JMeter
String plate = vars.get(&quot;plate&quot;);
String response = prev.getResponseDataAsString();

// Rutas de los archivos de salida
String fileFalse = &quot;C:/Users/alejandra.ospina/Documents/GitHub/testing-performance-ezytec/Diseño/Data/automatic_payment_false.txt&quot;;
String fileTrue = &quot;C:/Users/alejandra.ospina/Documents/GitHub/testing-performance-ezytec/Diseño/Data/automatic_payment_true.txt&quot;;
String fileNotFound = &quot;C:/Users/alejandra.ospina/Documents/GitHub/testing-performance-ezytec/Diseño/Data/users_not_found.txt&quot;;

// Analizar la respuesta JSON
def jsonResponse = new JsonSlurper().parseText(response);

// Construir el contenido a escribir
String contentToWrite = &quot;Request Plate: &quot; + plate + &quot;, Response: &quot; + response;

// Determinar el archivo adecuado basado en is_automatic_payment y message
String filePath;
if (jsonResponse.is_automatic_payment == false &amp;&amp; jsonResponse.message == &quot;Usuario encontrado&quot;) {
    filePath = fileFalse;
} else if (jsonResponse.is_automatic_payment == true &amp;&amp; jsonResponse.message == &quot;Usuario encontrado&quot;) {
    filePath = fileTrue;
} else {
    filePath = fileNotFound;
}

// Verificar si el archivo existe, si no, crea el archivo y escribe el encabezado
File file = new File(filePath);
boolean fileExists = file.exists();

try (FileWriter fw = new FileWriter(filePath, true);
     PrintWriter pw = new PrintWriter(fw)) {

    // Si el archivo no existe, escribe el encabezado
    if (!fileExists) {
        pw.println(&quot;Request Plate, Response&quot;);
    }

    // Escribir el contenido en el archivo
    pw.println(contentToWrite);
    log.info(&quot;Contenido guardado en el archivo: &quot; + filePath);

} catch (Exception e) {
    log.error(&quot;Error al guardar el contenido en el archivo: &quot; + filePath, e);
}



